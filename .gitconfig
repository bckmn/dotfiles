[includeIf "gitdir:~/work/"]
	path = ~/.gitconfig-work

[init]
	defaultBranch = main

[alias]
	st = status
	co = checkout
	cb = checkout -b
	update = !git checkout main && git pull
	cm = commit -m
	ca = commit --amend
	lon = log --oneline
	cp = cherry-pick
